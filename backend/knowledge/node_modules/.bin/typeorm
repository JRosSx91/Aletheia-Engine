#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/jross91/Escritorio/labs/Aletheia-Engine/node_modules/.pnpm/typeorm@0.3.26_better-sqlite3@11.10.0_reflect-metadata@0.2.2_ts-node@10.9.2_@swc+core@1_38bf4b1584ff525fecb5f0470bc87ae5/node_modules/typeorm/node_modules:/home/jross91/Escritorio/labs/Aletheia-Engine/node_modules/.pnpm/typeorm@0.3.26_better-sqlite3@11.10.0_reflect-metadata@0.2.2_ts-node@10.9.2_@swc+core@1_38bf4b1584ff525fecb5f0470bc87ae5/node_modules:/home/jross91/Escritorio/labs/Aletheia-Engine/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/home/jross91/Escritorio/labs/Aletheia-Engine/node_modules/.pnpm/typeorm@0.3.26_better-sqlite3@11.10.0_reflect-metadata@0.2.2_ts-node@10.9.2_@swc+core@1_38bf4b1584ff525fecb5f0470bc87ae5/node_modules/typeorm/node_modules:/home/jross91/Escritorio/labs/Aletheia-Engine/node_modules/.pnpm/typeorm@0.3.26_better-sqlite3@11.10.0_reflect-metadata@0.2.2_ts-node@10.9.2_@swc+core@1_38bf4b1584ff525fecb5f0470bc87ae5/node_modules:/home/jross91/Escritorio/labs/Aletheia-Engine/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../typeorm/cli.js" "$@"
else
  exec node  "$basedir/../typeorm/cli.js" "$@"
fi
